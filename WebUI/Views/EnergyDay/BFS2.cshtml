@using WebUI.App_LocalResources;

@{
    ViewBag.Title = @ReportEnergyResource.report_bfs2_title;
    ViewBag.Section = @ReportEnergyResource.report_bfs2_section;
    ViewBag.HierarchyMenu = @ViewBag.HierarchyMenu;
    Layout = "~/Views/Shared/_LayoutReportADD.cshtml";
}
@{
    TReport.TData.trObj[] obj = new TReport.TData.trObj[] { TReport.TData.trObj.dc2_dp9, TReport.TData.trObj.dc2_dp9_pci };
    TReport.TREntities.TREnergy.Report[] reports = new TReport.TREntities.TREnergy.Report[] { TReport.TREntities.TREnergy.Report.EnergyFlowDay, TReport.TREntities.TREnergy.Report.EnergyAVGDay, TReport.TREntities.TREnergy.Report.EnergyGranulDay };

}
@{ Html.RenderAction("EnergyDay", "EnergyDay", new { title = @ViewBag.Title, date = @DateTime.Parse(@ViewBag.dt), obj, reports }); }



